(setq mpz_2 (mpz "2"))
(for i 0 1 200 (progn
  (setq mpz_val (mpz_pow_i mpz_2 i))
  (outf "2^%ld = " i)
  (outf "%s\n" (mpz_tostr mpz_val))
))

(setq mpf_2 (mpf "2.0"))
(setq mpf_val (mpf "1.0"))
(for i 1 1 200 (progn
  (setq mpf_val (mpf_mul mpf_val mpf_2))
  (outf "2^%ld = " i)
  (outf "%s\n" (mpf_tostr mpf_val))
))

(setq mpf_precision 100)
(mpf_tostr (mpf_div (mpf (padl "1.0" mpf_precision))
                    (mpf (padl "3.0" mpf_precision))))

